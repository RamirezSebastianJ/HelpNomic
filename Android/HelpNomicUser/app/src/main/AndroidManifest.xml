<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.helpnomicuser">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/hn_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/hn_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar"

        android:usesCleartextTraffic="true">
        <!-- android:usesCleartextTraffic="true" Se agregÃ³ con el fin de permitir el trafico de datos por medio del protocolo HTTP-->
        <activity android:name=".ui.formularios.ConfirmacionSolicitudCredito"></activity>
        <activity android:name=".ui.formularios.FormularioSolicitudRenovacion" />
        <activity android:name=".ui.formularios.FormularioSolicitudCreditoActivity" />
        <activity
            android:name=".MenuUsuarioActivity"
            android:label="@string/title_activity_menu_usuario"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>